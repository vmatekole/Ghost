{{!< default}}

{{#post}}

    {{> post-meta}}

    <header class="animated fadeIn">
        <div class="container" data-stellar-ratio="0.75">
            {{> title}}
            <div class="sidebar text-right meta">
                <div class="published animated fadeInUp">
                    <strong>Last Updated</strong>
                    <time datetime="{{date format='YYYY-MM-DD'}}">{{date format='DD MMM YYYY, h:mm:ss a'}}</time>
                </div>
            </div>
        </div>
    </header>

{{! Temporary indentation shift for Handlebars issue }}
{{> content-custom}}

{{/post}}
